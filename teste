_new_ _Integer_ TESTE; 
_new_ _Integer_ VAR;
_new_ _Char_ CARACTERE;
_new_ _Float_ NADA;

VAR = 10 * 1 + 1;
TESTE = 5;
NADA = 5 + 4.2 + NADA + VAR;
CARACTERE = 'c';

_print_ "Teste de linguagem";

_print_ VAR;

_reader_ VAR;

_if_ 10 :: 10 || 10 >> 1 $> 
    _print_ "Vari√°vel maior que dez";
$<;

_repeater_ 10 << VAR $>
    VAR = VAR - 10;
$<;

_for_ 1, 10, I $> 
    TESTE = TESTE * 2;
$<;

_print_ TESTE;
